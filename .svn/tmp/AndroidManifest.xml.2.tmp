        <activity
            android:name="net.zhomi.negotiation.activity.login.RegisterActivity"
            android:theme="@android:style/Theme.NoTitleBar" />
        <activity
            android:name="net.zhomi.negotiation.activity.login.RegisterAuthCodeActivity"
            android:theme="@android:style/Theme.NoTitleBar" />
        <activity
