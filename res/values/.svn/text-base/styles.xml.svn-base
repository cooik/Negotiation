<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <style name="MMFontTitleInList">
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">#454545</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="MMListCatalog">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#6c6c6c</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">#f6fbff</item>
        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:paddingTop">2.0dip</item>
        <item name="android:paddingBottom">2.0dip</item>
        <item name="android:layout_width">fill_parent</item>
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="main_navi_radiobutton">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:textSize">@dimen/main_navi_radiobutton_size</item>
        <item name="android:textColor">@color/main_navi_radiobutton</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:paddingTop">@dimen/main_navi_radiogroup_padding</item>
        <item name="android:paddingBottom">@dimen/main_navi_radiogroup_padding</item>
    </style>
    <!-- 注册和登录的输入框和按钮 -->
    <style name="register_login_bg">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/public_title_height</item>
        <item name="android:layout_marginLeft">@dimen/public_interval_left</item>
        <item name="android:layout_marginRight">@dimen/public_interval_right</item>
        <item name="android:layout_marginTop">@dimen/public_interval_top</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="CalendarTheme">
        <item name="calendarViewStyle">@style/CalendarStyle</item>
    </style>

    <style name="CalendarStyle">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="public_line_fill_wrap">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@android:color/darker_gray</item>
    </style>

    <!-- 卡片式菜单 -->
    <style name="PopupWindowAnimation">
        <item name="android:windowEnterAnimation">@anim/popshow_anim</item>
        <item name="android:windowExitAnimation">@anim/pophidden_anim</item>
    </style>

    <style name="normaltext"></style>

    <style name="wrap_height">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#505050</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="PopupAnimation" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/pop_enter</item>
        <item name="android:windowExitAnimation">@anim/pop_out</item>
    </style>
    <!-- 业态选择style -->
    <style name="cate_query_spinner">
        <item name="android:layout_width">@dimen/cate_content_query_spinner_width</item>
        <item name="android:background">@drawable/selector_bg_spinner</item>
        <item name="android:layout_height">@dimen/cate_content_item_height</item>
    </style>
    <!-- 品牌详情 -->
    <style name="activity_brand_navi_button">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:button">@null</item>
        <item name="android:textSize">@dimen/brand_title_text_size</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:paddingTop">@dimen/main_navi_button_padding</item>
        <item name="android:paddingBottom">@dimen/main_navi_button_padding</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:background">@null</item>
    </style>

    <style name="fill">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="fragment_brand_files_text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="fragment_brand_files_CardRelative">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">2dp</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="fragment_brand_files_CardTv_left">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:padding">15dp</item>
        <item name="android:textColor">@color/text_normal</item>
    </style>

    <style name="fragment_brand_files_CardTv_Right">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:padding">15dp</item>
        <item name="android:textColor">@color/text_normal</item>
    </style>

</resources>